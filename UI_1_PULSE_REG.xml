<?xml version="1.0" encoding="UTF-8"?>
<prog>
  <head>
    <include>MarcuscTest</include>
  </head>
  <prog_body>
    <function name="main">
      <body>
        <comment>测试IO</comment>
		<assignment type="io_assignment">
			<lvalue>
			  <element type="uo">
				<element type="num">1</element>
			  </element>
			</lvalue>
			<rvalue>
			  <element type="io_val">ON</element>
			</rvalue>
		</assignment>
        <assignment type="assignment">
          <lvalue>
            <element type="ro">
              <element type="num">1</element>
            </element>
          </lvalue>
          <rvalue>
			  <element type="io_val">pulse</element>
			  <element type="num">10</element>
          </rvalue>
        </assignment>
		<instruction type="wait">
			   <argument name="timeout" type="num" unit="sec">1</argument>
		</instruction>
        <assignment type="assignment">
          <lvalue>
            <element type="ro">
              <element type="num">2</element>
            </element>
          </lvalue>
          <rvalue>
			  <element type="register">
				<element type="num">2</element>
			  </element>
          </rvalue>
        </assignment>
        <comment>测试结束</comment>
        <end name="prog"/>
      </body>
    </function>
  </prog_body>
</prog>
