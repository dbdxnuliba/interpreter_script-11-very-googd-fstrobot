<?xml version="1.0" encoding="utf-8"?>

<prog>
  <head/>
  <prog_body>
    <function name="main">
      <body>
        <nop/>
        <logical type="while">
          <determine>
            <element type="num">1</element>
            <element type="boolean_operation">=</element>
            <element type="num">1</element>
          </determine>
          <logical_body>
            <nop/>
            <instruction type="move_joint">
              <argument name="pose" type="pose">
                <element type="num">1</element>
              </argument>
              <argument name="velocity" unit="%" type="num">100</argument>
              <argument name="fine" type="num"/>
            </instruction>
            <instruction type="move_joint">
              <argument name="pose" type="pose">
                <element type="num">6</element>
              </argument>
              <argument name="velocity" unit="%" type="num">100</argument>
              <argument name="fine" type="num"/>
            </instruction>
            <instruction type="wait">
              <argument name="timeout" type="num" unit="sec">1</argument>
            </instruction>
            <instruction type="move_circle">
              <argument name="cirpose" type="pose">
                <element type="num">2</element>
              </argument>
              <argument name="topose" type="pose">
                <element type="num">3</element>
              </argument>
              <argument name="velocity" unit="mm/s" type="num">4000</argument>
              <argument name="smooth_velocity" type="num">50</argument>
            </instruction>
            <instruction type="move_circle">
              <argument name="cirpose" type="pose">
                <element type="num">3</element>
              </argument>
              <argument name="topose" type="pose">
                <element type="num">4</element>
              </argument>
              <argument name="velocity" unit="mm/s" type="num">4000</argument>
              <argument name="fine" type="num"/>
            </instruction>
          </logical_body>
        </logical>
        <end name="prog"/>
      </body>
    </function>
  </prog_body>
</prog>
