<?xml version="1.0" encoding="utf-8"?>

<prog>
  <head/>
  <prog_body>
    <function name="main">
      <body>
        <nop/>
		<instruction type="move_line">
			<argument name="pose" type="pose">
					<element type="num">5</element>
			</argument>
			<argument name="velocity" type="num" unit="mm/s">2000</argument>
			<argument name="smooth_velocity" type="num">100</argument>
			<argument name="acceleration" type="num" unit="%">40</argument>
		</instruction>
        <instruction type="wait">
          <argument name="timeout" type="num" unit="sec">2</argument>
        </instruction>
        <end name="prog"/>
      </body>
    </function>
  </prog_body>
</prog>
